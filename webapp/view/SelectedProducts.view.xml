					<m:Text text="{Plant}" wrapping="false"/>
				</template>
			</Column>
			<Column sortProperty="StorageLocation" filterProperty="StorageLocation">
				<m:Text text="{i18n>StorageLocation}"/>
				<template>
					<m:Text text="{StorageLocation}" wrapping="false"/>
				</template>
			</Column>
			<Column sortProperty="Stock" filterProperty="Stock">
				<m:Text text="{i18n>Stock}"/>
				<template>
					<m:Text text="{Stock}" wrapping="false"/>
				</template>
			</Column>
			<Column sortProperty="Supplier" filterProperty="Supplier">
				<m:Text text="{i18n>Supplier}"/>
				<template>
					<m:Text text="{Supplier}" wrapping="false"/>
				</template>
			</Column>
			<Column sortProperty="Price" filterProperty="Price">
				<m:Text text="{i18n>Price}"/>
				<template>
					<m:Text text="{Price}" wrapping="false"/>
				</template>
			</Column>
		</columns>
	</Table>
</mvc:View>